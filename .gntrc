# [colors]
# # The RGB values range in [0, 1000]
# black = 0; 0; 0
# red = 1000; 0; 0
# green = 0; 1000; 0
# blue = 1000; 500; 500
# white = 1000; 1000; 1000
# gray = 700; 700; 700
# darkgray = 256; 256; 256
#
# Note: not possible to define new color names, e.g. yellow = 256; 256; 256
# Note: ncurses must detect that the terminal is capable of redefining colors
#       only TERM=xterm-256color seems to do it, but there is not need to redefine colors
# Note: even setting TERM to xterm-256color does not work within screen (in regard to redefining colors)
# Note: best setting for TERM is screen-256color (this does not produce visual artifacts - no leftover underscores)

[colorpairs]
normal = 244; 234
highlight = 37; 235
highlightd = 254; 235
shadow = 232; 234
title = 254; 235
titled = 244; 235
text = 254; 234
disabled = 245; 234
# urgent = 234; 136
urgent = 136; 235

[general]
color-available = 136; 234
color-idle = 94; 234
color-away = 27; 234
color-offline = 124; 234
color-message-sent = 37; 234
# maybe 160 for nickname of the received message (brighter red)
color-message-received = 124; 234
color-message-highlight = 234; 136
color-message-action = 27; 234
color-timestamp = 27; 234

#shadow = 0
# There is experimental mouse support (should work in xterm-256color or screen-256color terminals, even rxvt looks to work)
mouse = 1
# To use some custom window-manager
#wm = /usr/lib/gnt/irssi.so
# This is needed only when really changing terminal colors (not when using from the available colors via colorpairs)
customcolor = 0

[GntWM::binding]
c-n = window-next
c-p = window-prev
c-r = start-resize
c-t = start-move

